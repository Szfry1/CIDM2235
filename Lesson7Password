using System;
namespace lesson7Fundamentals
{
class Program
 {
  public static void Main (string[] args) 
  {
    while(true)
    {
      Console.WriteLine("What is your password?");
      var input = Console.ReadLine();

      if (input == "Laugh100*")
      {
        Console.WriteLine("Access Granted");
        break;
      }
      else {
        Console.WriteLine("Invalid password");
        continue;
      }
    }
  }//end program
 }//end main
}//end namespace
